# DÃ©claration de la fonction ajouter_elt
def ajouter_elt(lst=[0, 1, 2], elt=3):
    lst.append(elt)
    return lst
print(ajouter_elt())

print(ajouter_elt())
print(ajouter_elt())

lst = [0, 1, 2]
print(id(lst))
lst = ajouter_elt(lst)
print(id(lst))
lst = ajouter_elt(lst)
print(id(lst))

def ajouter_carac(ch="abc", elt="d"):
    return ch + elt

print(ajouter_carac())

print(ajouter_carac())
print(ajouter_carac())
